<%- include('header'); -%>
  <%- include("nav"); -%>
<div class="detail">
<article>
  <h3><%=post.title %></h3>
   <img src="/images/<%=post.image %>" alt="">
  <% for(let t=0; t<post.tags.length; t++) { %>
    <span><%=post.tags[t] %></span>
  <% } // end forloop %>
  <address class="author"><%=post.author %></address>
  <p><%=post.period %></p>
  <p><%=post.excerpt %></p>

</article>
  </div>
<%- include('footer'); -%>
